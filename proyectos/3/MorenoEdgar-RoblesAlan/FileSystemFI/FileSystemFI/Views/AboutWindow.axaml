        <Styles>
            <Style Selector="Label">
                <Setter Property="VerticalAlignment" Value="Center"></Setter>
                <Setter Property="HorizontalAlignment" Value="Center"></Setter>
            </Style>
        </Styles>
    </Window.Styles>
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"
                Orientation="Vertical">
        <Label
            FontWeight="Bold"
            FontSize="20">
            Facultad de Ingeniería - UNAM
        </Label>
        <Separator></Separator>
        <Label
            FontWeight="Bold"
            FontSize="16">
            Proyecto 3 - (Micro) sistema de archivos
        </Label>
        <Label
            FontWeight="Bold"
            FontSize="16">
            Sistemas Operativos - Semestre 2024-1
        </Label>
        <Separator></Separator>
        <Label>Moreno Chalico Edgar Ulises</Label>
        <Label>Robles Reyes Alan</Label>
    </StackPanel>
</Window>